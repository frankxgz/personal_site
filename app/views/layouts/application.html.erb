<!DOCTYPE html>
<html>
  <head>
    <title>Guangzhi | <%= yield(:title) %></title>
    <%= stylesheet_link_tag    "application", media: "all" %>
    <%= javascript_include_tag "application" %>
    
    <%= stylesheet_link_tag 'jquery-ui/css/flick/jquery-ui-1.8.21.custom.css',  media: 'screen' %>
    <%= javascript_include_tag 'jquery-ui/js/jquery-1.7.2.min.js'%>
    <%= javascript_include_tag 'jquery-ui/js/jquery-ui-1.8.21.custom.min.js'%>
    
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>    
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= yield %>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>